<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>DockingManagerMVVMCaliburnMicro</AssemblyName>
    <RootNamespace>DockingManagerMVVMCaliburnMicro</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="DockingAdapterMVVM\DockingAdapterMVVM_NETCore.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <Compile Remove="DockingAdapterMVVM\**" />
    <EmbeddedResource Remove="DockingAdapterMVVM\**" />
    <None Remove="DockingAdapterMVVM\**" />
    <Page Remove="DockingAdapterMVVM\**" />
    <Resource Remove="DockingAdapterMVVM\**" />
    <Compile Remove="Sample.cs" />
    <Page Remove="Sample.xaml" />
    <Resource Include="App.ico" />
    <Resource Include="Sample.cs">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Content Include="Sample.xaml">
      <Generator>MSBuild:Compile</Generator>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Reference Include="Caliburn.Micro">
      <HintPath>Library\Caliburn.Micro.dll</HintPath>
    </Reference>
    <PackageReference Include="Syncfusion.Edit.Wpf" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Grid.Wpf"Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.GridCommon.Wpf" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.PropertyGrid.Wpf" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.2.0.34" />
    <Reference Include="System.ComponentModel.Composition">
      <HintPath>Library\System.ComponentModel.Composition.dll</HintPath>
    </Reference>
    <Reference Include="System.ComponentModel.Composition.Initialization">
      <HintPath>Library\System.ComponentModel.Composition.Initialization.dll</HintPath>
    </Reference>
    <Reference Include="System.Windows.Interactivity">
      <HintPath>Library\System.Windows.Interactivity.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>