<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>DocumentEditor</AssemblyName>
    <RootNamespace>DocumentEditor</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <None Remove="Assets\CustomDictionary.dic" />
    <None Remove="Assets\GettingStarted.docx" />
    <Resource Include="App.ico" />
    <Content Include="Assets\CustomDictionary.dic">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <EmbeddedResource Include="Assets\GettingStarted.docx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <PackageReference Include="Syncfusion.Compression.Base" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.DocIO.Base" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.SfRichTextBoxAdv.WPF.Classic" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.SfRichTextRibbon.WPF" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="17.2.0.34" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.2.0.34" />
</Project>